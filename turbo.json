{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "consoledot-rhosak#build": {
      "env": [
        "API_URL"
      ]
    },
    "ui#build": {
      "env": [
        "JEST_WORKER_ID"
      ]
    },
    "lint": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "test-storybook:ci": {
      "dependsOn": [
      ],
      "outputs": []
    }
  },
  "globalEnv": [
    "GITHUB_TOKEN",
    "NODE_ENV"
  ]
}
